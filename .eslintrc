{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "react/jsx-filename-extension": 0,
    "arrow-parens": 0,
    "react/require-default-props": 0,
    "react/no-typos": 0,
    "indent": ["error", 2, { "SwitchCase": 1 }]
  }
}
